package com.company;

public class Main {

    public static void main(String[] args) {
        int a = 44;
        int b = 18;
        int c = -23;
        if (a > b) {
            if (b > c) {
                System.out.println(a * a + b * b);
            } else {
                System.out.println(a * a + c * c);
            }
        } else if (a > c) {
            System.out.println(a * a + b * b);
        } else {
            System.out.println(b * b + c * c);
        }
    }


}

